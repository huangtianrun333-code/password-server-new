{
  "version": 3,
  "builds": [
    {
      "src": "build.js",
      "use": "@vercel/static-build"
    }
  ],
  "buildCommand": "node build.js",
  "outputDirectory": ".vercel/output",
  "cache": [
    "node_modules/**/*",
    "package-lock.json",
    "build.js",
    "api/**/*.js",
    "lib/**/*.js"
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
